{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install && cd client && npm install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.2",
      "maxDuration": 60
    },
    "api/data-ops/index.py": {
      "runtime": "@vercel/python@1.5.1",
      "memory": 1024,
      "maxDuration": 60,
      "includeFiles": "python-service/**"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
